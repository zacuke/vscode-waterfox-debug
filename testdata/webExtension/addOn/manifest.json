{

	"description": "A WebExtension used for testing vscode-firefox-debug",
	"manifest_version": 2,
	"name": "Test",
	"version": "1.0",
	"homepage_url": "https://github.com/hbenl/vscode-firefox-debug",
	"applications": {
		"gecko": {
			"id": "{12345678-1234-1234-1234-123456781234}"
		}
	},
	"background": {
		"scripts": [ "backgroundscript.js" ]
	},
	"content_scripts": [
		{
			"matches": [ "file:///*" ], 
			"js": [ "contentscript.js" ]
		}
	]
}
